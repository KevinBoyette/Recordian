<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>

<GridPane hgap="10" prefHeight="69.0" prefWidth="358.0" vgap="10" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.kylewill.controller.databaseitemcontroller.EditLocationController">
    <padding><Insets bottom="25" left="25" right="25" top="25" /></padding>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="Location Name" />
      <Button fx:id="saveButton" mnemonicParsing="false" text="Save" GridPane.rowIndex="5" />
      <Button fx:id="cancelButton" mnemonicParsing="false" text="Cancel" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <TextField fx:id="locationName" GridPane.columnIndex="1" />
      <Label text="Address" GridPane.rowIndex="1" />
      <Label text="City" GridPane.rowIndex="2" />
      <Label text="State" GridPane.rowIndex="3" />
      <Label text="Zip" GridPane.rowIndex="4" />
      <TextField fx:id="locationAddress" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <TextField fx:id="locationCity" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <TextField fx:id="locationState" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <TextField fx:id="locationZipCode" GridPane.columnIndex="1" GridPane.rowIndex="4" />
   </children>
</GridPane>
