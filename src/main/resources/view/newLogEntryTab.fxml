<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.NewLogEntryTabController">
    <VBox maxHeight="-Infinity" maxWidth="-Infinity">
      <GridPane hgap="10.0" vgap="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="-Infinity" />
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints />
        </rowConstraints>
         <children>
                <Label text="Hours Worked" />
                <TextField fx:id="hours" maxHeight="-Infinity" maxWidth="-Infinity" GridPane.columnIndex="1" />
            <Label alignment="CENTER_RIGHT" maxWidth="1.7976931348623157E308" text="Date" GridPane.rowIndex="1" />
              <DatePicker fx:id="theDatePicker" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Label alignment="TOP_RIGHT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="Comments" GridPane.rowIndex="2" />
                <TextArea fx:id="comments" maxWidth="-Infinity" minHeight="-Infinity" prefHeight="53.0" prefWidth="200.0" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
        <GridPane alignment="CENTER" hgap="10" vgap="10">
            <children>
                <Label text="Location" GridPane.rowIndex="1" />
                <ChoiceBox fx:id="locationChoiceBox" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                <Button fx:id="addLocationButton" mnemonicParsing="false" text="Add" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                <Button fx:id="editLocationButton" mnemonicParsing="false" text="Edit" GridPane.columnIndex="3" GridPane.rowIndex="1" />
                <Button fx:id="deleteLocationButton" mnemonicParsing="false" text="Delete" GridPane.columnIndex="4" GridPane.rowIndex="1" />
                <CheckBox fx:id="locationDefaultCheckbox" mnemonicParsing="false" text="Default" GridPane.columnIndex="5" GridPane.rowIndex="1" />
                <Label text="Company" GridPane.rowIndex="2" />
                <ChoiceBox fx:id="companyChoiceBox" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                <Button fx:id="addCompanyButton" mnemonicParsing="false" text="Add" GridPane.columnIndex="2" GridPane.rowIndex="2" />
                <Button fx:id="editCompanyButton" mnemonicParsing="false" text="Edit" GridPane.columnIndex="3" GridPane.rowIndex="2" />
                <Button fx:id="deleteCompanyButton" mnemonicParsing="false" text="Delete" GridPane.columnIndex="4" GridPane.rowIndex="2" />
                <CheckBox fx:id="companyDefaultCheckbox" mnemonicParsing="false" text="Default" GridPane.columnIndex="5" GridPane.rowIndex="2" />
                <Label text="Supervisor" GridPane.rowIndex="3" />
                <ChoiceBox fx:id="supervisorChoiceBox" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                <Button fx:id="addSupervisorButton" mnemonicParsing="false" text="Add" GridPane.columnIndex="2" GridPane.rowIndex="3" />
                <Button fx:id="editSupervisorButton" mnemonicParsing="false" text="Edit" GridPane.columnIndex="3" GridPane.rowIndex="3" />
                <Button fx:id="deleteSupervisorButton" mnemonicParsing="false" text="Delete" GridPane.columnIndex="4" GridPane.rowIndex="3" />
                <CheckBox fx:id="supervisorDefaultCheckbox" mnemonicParsing="false" text="Default" GridPane.columnIndex="5" GridPane.rowIndex="3" />
                <Label fx:id="errorLabel" text="Please enter amount of hours worked" textFill="#eb0000" visible="false" GridPane.columnSpan="2147483647" GridPane.halignment="CENTER" GridPane.rowIndex="4" />
                <Button fx:id="submit" text="Submit" GridPane.columnSpan="2147483647" GridPane.halignment="CENTER" GridPane.rowIndex="5" />
            </children>
            <columnConstraints>
                <ColumnConstraints />
                <ColumnConstraints maxWidth="-Infinity" prefWidth="200.0" />
                <ColumnConstraints />
                <ColumnConstraints />
                <ColumnConstraints />
                <ColumnConstraints maxWidth="-Infinity" minWidth="72.0" prefWidth="72.0" />
            </columnConstraints>
            <rowConstraints>
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
                <RowConstraints />
            <RowConstraints />
            </rowConstraints>
        </GridPane>
    </VBox>
</VBox>
